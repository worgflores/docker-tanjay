<schema  adapter="market">
	<element tablename="market_cluster">
		<field name="objid" primary="true" prefix="MKTCLU"/>
		<field name="market_objid" visible="false"/>
		<field name="market_name" searchable="true" indexed="true"/>
		<field name="market_barangay_objid" />
		<field name="market_barangay_name" />

		<field name="name" required="true" caption="Cluster Name" searchable="true" indexed="true"/>
		<field name="description" />
	</element>
</schema>
